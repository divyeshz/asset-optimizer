{
    "name": "divyeshz/asset-optimizer",
    "description": "A library for optimizing images, videos, and audio, with thumbnail creation and storage features.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AssetOptimizer\\": "src/"
        }
    }, 
    "extra": {
        "laravel": {
            "providers": [
                "AssetOptimizer\\AssetOptimizerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Divyesh Mepal",
            "email": "divyeshm@zignuts.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "intervention/image": "^3.6",
        "php-ffmpeg/php-ffmpeg": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.2"
    },
    "license": "MIT"
}
